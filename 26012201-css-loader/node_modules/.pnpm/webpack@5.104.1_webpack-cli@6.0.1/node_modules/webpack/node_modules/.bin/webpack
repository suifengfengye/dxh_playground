#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jianfehuang/Downloads/work-max/dxh_playground/26012201-css-loader/node_modules/.pnpm/webpack@5.104.1_webpack-cli@6.0.1/node_modules/webpack/bin/node_modules:/Users/jianfehuang/Downloads/work-max/dxh_playground/26012201-css-loader/node_modules/.pnpm/webpack@5.104.1_webpack-cli@6.0.1/node_modules/webpack/node_modules:/Users/jianfehuang/Downloads/work-max/dxh_playground/26012201-css-loader/node_modules/.pnpm/webpack@5.104.1_webpack-cli@6.0.1/node_modules:/Users/jianfehuang/Downloads/work-max/dxh_playground/26012201-css-loader/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jianfehuang/Downloads/work-max/dxh_playground/26012201-css-loader/node_modules/.pnpm/webpack@5.104.1_webpack-cli@6.0.1/node_modules/webpack/bin/node_modules:/Users/jianfehuang/Downloads/work-max/dxh_playground/26012201-css-loader/node_modules/.pnpm/webpack@5.104.1_webpack-cli@6.0.1/node_modules/webpack/node_modules:/Users/jianfehuang/Downloads/work-max/dxh_playground/26012201-css-loader/node_modules/.pnpm/webpack@5.104.1_webpack-cli@6.0.1/node_modules:/Users/jianfehuang/Downloads/work-max/dxh_playground/26012201-css-loader/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/webpack.js" "$@"
else
  exec node  "$basedir/../../bin/webpack.js" "$@"
fi
